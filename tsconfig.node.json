{
  "include": [
    "src/**/*",
    "./locale/**/*",
    "./gatsby/**/*",
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "module": "commonJS",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "es2016"],
    "esModuleInterop": true
  }
}
